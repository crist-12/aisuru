{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include" : ["./"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules|docs)"
    },
    "opts" : {
        "recurse" : "true",
        "destination": "doc"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}

